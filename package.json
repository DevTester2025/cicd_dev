  "description": "CloudMatiq API",
  "main": "index.js",
  "scripts": {
    "start": "cd build && node main",
    "compile-prod": "node build.js --env=prod && backpack build",
    "compile-test": "node build.js --env=test && backpack build",
    "compile-demo": "node build.js --env=demo && backpack build",
    "compile": "node build.js --env=dev && backpack build",
    "serve": "node build.js --env=dev && backpack dev | pino -o 'short'",
    "debug": "node build.js --env=dev && backpack dev --inspect-brk | pino -o 'short'",
    "test": "node build.js --env=dev && mocha test/**/*.ts --exit"
